@using ProjectAdmin.Models;
<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
 
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>Online Pharmacy - Index</title>
    <!--
        CSS
        ============================================= -->
    <link rel="stylesheet" href="~/ProductAssets/css/linearicons.css">
    <link rel="stylesheet" href="~/ProductAssets/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/ProductAssets/css/themify-icons.css">
    <link rel="stylesheet" href="~/ProductAssets/css/bootstrap.css">
    <link rel="stylesheet" href="~/ProductAssets/css/owl.carousel.css">
    <link rel="stylesheet" href="~/ProductAssets/css/nice-select.css">
    <link rel="stylesheet" href="~/ProductAssets/css/nouislider.min.css">
    <link rel="stylesheet" href="~/ProductAssets/css/ion.rangeSlider.css" />
    <link rel="stylesheet" href="~/ProductAssets/css/ion.rangeSlider.skinFlat.css" />
    <link rel="stylesheet" href="~/ProductAssets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/ProductAssets/css/main.css">
    <style>
        .addiy {
            background: linear-gradient(90deg, #9da2a5 0%, #ffffff3d 100%) !important;
        }

        .banner-area .active-banner-slider .owl-nav .owl-prev, .banner-area .active-banner-slider .owl-nav .owl-next {
            display: none !important;
        }
    </style>
</head>

<body>

    <!-- Start Header Area -->
    <header class="header_area sticky-header">
        <div class="main_menu">
            <div class="container-fluid " style="padding-left:0px; padding-right:0px;">
                <nav class="navbar navbar-expand-lg navbar-light addiy p-3">
                    <a class="navbar-brand" href="@Url.Action("Index","Home")"><i class="fa fa-medkit" aria-hidden="true"></i> Online Pharmacy </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="nav navbar-nav navbar-right ml-auto">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="ti-bag"></i>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    @if (Session["cart"] != null)
                                    {
                                        foreach (Item item in (List<Item>)Session["cart"])
                                        {
                                            <a href="@Url.Action("RemoveFromCart", "Home", new { productId=item.product.ProductId})" class="dropdown-item">@item.product.ProductName (@item.Quantity) &nbsp;<i class="fa fa-times"></i></a>
                                        }
                                    }
                                </div>
                            </li>
                            @if (User.Identity.IsAuthenticated)
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Logout","Admin")">Logout</a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Login","Admin")">Login</a>
                                </li>
                            }

                            @if (User.Identity.IsAuthenticated)
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Checkout", "Home")">Checkout</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Career", "Home")">Career</a>
                                </li>
                            }
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                                    Products
                                </a>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" href="@Url.Action("Capsule","Home")">Capsules</a>
                                    <a class="dropdown-item" href="@Url.Action("Tablet","Home")">Tablets</a>
                                    <a class="dropdown-item" href="@Url.Action("Syrup","Home")">Syrups</a>

                                </div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("About","Home")">About Us</a>
                            </li>
                   
                            @*<li class="nav-item">
            <a class="nav-link" href="@Url.Action("Login","Admin")">Quote Us</a>
        </li>*@
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Contact","Home")">Contact Us</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
    </header>
    <!-- End Header Area -->
    <!-- start banner Area -->
    <section class="banner-area">
        <div class="container">
            <div class="row fullscreen align-items-center justify-content-start">
                <div class="col-lg-12">
                    <div class="active-banner-slider owl-carousel">
                        <!-- single-slide -->
                        <div class="row single-slide align-items-center d-flex">
                            <div class="col-lg-5 col-md-6">
                                <div class="banner-content">
                                    <h1>Adapt To<br>Every Situation.</h1>
                                    <p>
                                        Whether Pivoting your business or advertising a new option, make sure every passer by see it. 
                                    </p>
                                </div>
                            </div>
                            <div class="col-lg-7">
                                <div class="banner-img">
                                    @*<img class="img-fluid" src="~/images/DesignStudioGraphic.png" alt="">*@
                                </div>
                            </div>
                        </div>
                        <!-- single-slide -->
                        <div class="row single-slide">
                            <div class="col-lg-5">
                                <div class="banner-content">
                                    <h1>Building A Brand To Be<br>Proud Of.</h1>
                                    <p>
                                        For a look that's all you, simplicity can make a strong statement.
                                    </p>
                                </div>
                            </div>
                            <div class="col-lg-7">
                                <div class="banner-img">
                                    @*<img class="img-fluid" src="~/images/197-1976609_available-paper-stock-visiting-card (1).png" alt="">*@
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End banner Area -->

    @RenderBody()

    <!-- start footer Area -->
    <footer class="footer-area section_gap">
        <div class="container">
            <div class="row">
                <div class="col-lg-3  col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6>About Us</h6>
                        <p>
                            Pharmacy is an innovative pharmacy-led healthcare group.
                        </p>
                    </div>
                </div>
                <div class="col-lg-5  col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6>Newsletter</h6>
                        <p>Stay update with our latest</p>
                        <div class="" id="mc_embed_signup">

                            <form target="_blank" novalidate="true" action="https://spondonit.us12.list-manage.com/subscribe/post?u=1462626880ade1ac87bd9c93a&amp;id=92a4423d01"
                                  method="get" class="form-inline">

                                <div class="d-flex flex-row">

                                    <input class="form-control" name="EMAIL" placeholder="Enter Email" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Email '"
                                           required="" type="email">


                                    <button class="btn btn-secondary"><i class="fa fa-long-arrow-right" aria-hidden="true"></i></button>
                                    <div style="position: absolute; left: -5000px;">
                                        <input name="b_36c4fd991d266f23781ded980_aefe40901a" tabindex="-1" value="" type="text">
                                    </div>

                                    <!-- <div class="col-lg-4 col-md-4">
                                                <button class="bb-btn btn"><span class="lnr lnr-arrow-right"></span></button>
                                            </div>  -->
                                </div>
                                <div class="info"></div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6>Follow Us</h6>
                        <p>Let us be social</p>
                        <div class="footer-social d-flex align-items-center">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                           <a href="#"><i class="fa fa-instagram"></i> </a>

                           
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-bottom d-flex justify-content-center align-items-center flex-wrap">
                <p class="footer-text m-0">
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | Powered By Pharmacy
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </p>
            </div>
        </div>
    </footer>
    <!-- End footer Area -->

    <script src="~/ProductAssets/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
            crossorigin="anonymous"></script>
    <script src="~/ProductAssets/js/vendor/bootstrap.min.js"></script>
    <script src="~/ProductAssets/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/ProductAssets/js/jquery.nice-select.min.js"></script>
    <script src="~/ProductAssets/js/jquery.sticky.js"></script>
    <script src="~/ProductAssets/js/nouislider.min.js"></script>
    <script src="~/ProductAssets/js/countdown.js"></script>
    <script src="~/ProductAssets/js/jquery.magnific-popup.min.js"></script>
    <script src="~/ProductAssets/js/owl.carousel.min.js"></script>
    <!--gmaps Js-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
    <script src="~/ProductAssets/js/gmaps.min.js"></script>
    <script src="~/ProductAssets/js/main.js"></script>
</body>

</html>